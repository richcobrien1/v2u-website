<svg width="200" height="200" viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Neural Network Vault Design -->
  <defs>
    <linearGradient id="cortexGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1E3A8A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#7C3AED;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Vault/Shield Outline -->
  <path d="M 100 20 
           L 160 50 
           L 160 120 
           Q 160 150 140 165 
           Q 120 180 100 180 
           Q 80 180 60 165 
           Q 40 150 40 120 
           L 40 50 
           Z" 
        fill="url(#cortexGradient)" 
        opacity="0.1" 
        stroke="url(#cortexGradient)" 
        stroke-width="3"/>
  
  <!-- Central Lock Icon -->
  <circle cx="100" cy="100" r="25" fill="none" stroke="url(#cortexGradient)" stroke-width="3"/>
  <rect x="90" y="110" width="20" height="25" rx="2" fill="url(#cortexGradient)"/>
  <circle cx="100" cy="105" r="5" fill="#F59E0B"/>
  
  <!-- Neural Network Nodes -->
  <circle cx="70" cy="60" r="4" fill="#7C3AED" filter="url(#glow)"/>
  <circle cx="130" cy="60" r="4" fill="#7C3AED" filter="url(#glow)"/>
  <circle cx="50" cy="100" r="4" fill="#7C3AED" filter="url(#glow)"/>
  <circle cx="150" cy="100" r="4" fill="#7C3AED" filter="url(#glow)"/>
  <circle cx="70" cy="140" r="4" fill="#7C3AED" filter="url(#glow)"/>
  <circle cx="130" cy="140" r="4" fill="#7C3AED" filter="url(#glow)"/>
  
  <!-- Connection Lines -->
  <line x1="70" y1="60" x2="100" y2="75" stroke="#7C3AED" stroke-width="1.5" opacity="0.5"/>
  <line x1="130" y1="60" x2="100" y2="75" stroke="#7C3AED" stroke-width="1.5" opacity="0.5"/>
  <line x1="50" y1="100" x2="75" y2="100" stroke="#7C3AED" stroke-width="1.5" opacity="0.5"/>
  <line x1="150" y1="100" x2="125" y2="100" stroke="#7C3AED" stroke-width="1.5" opacity="0.5"/>
  <line x1="70" y1="140" x2="100" y2="125" stroke="#7C3AED" stroke-width="1.5" opacity="0.5"/>
  <line x1="130" y1="140" x2="100" y2="125" stroke="#7C3AED" stroke-width="1.5" opacity="0.5"/>
  
  <!-- Outer Nodes (Property Types) -->
  <g opacity="0.7">
    <circle cx="100" cy="30" r="3" fill="#10B981"/>
    <circle cx="170" cy="80" r="3" fill="#10B981"/>
    <circle cx="170" cy="120" r="3" fill="#10B981"/>
    <circle cx="100" cy="170" r="3" fill="#10B981"/>
    <circle cx="30" cy="120" r="3" fill="#10B981"/>
    <circle cx="30" cy="80" r="3" fill="#10B981"/>
  </g>
</svg>
